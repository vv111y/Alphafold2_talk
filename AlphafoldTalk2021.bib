@article{travers18_oppor_obstac_deep_learn_biolog_medic,
  author =       {{Ching Travers} and {Himmelstein Daniel S.} and
                  {Beaulieu-Jones Brett K.} and {Kalinin Alexandr A.} and {Do
                  Brian T.} and {Way Gregory P.} and {Ferrero Enrico} and
                  {Agapow Paul-Michael} and {Zietz Michael} and {Hoffman Michael
